

int led1 = D7; 


void setup() {



  
  pinMode(led1, OUTPUT);

}

void loop() {
  // To blink the LED, first we'll turn it on...
  //For Z
  digitalWrite(led1, HIGH);
  delay(2000);
  digitalWrite(led1, LOW);
  delay(500);
  digitalWrite(led1, HIGH);
  delay(2000);
  digitalWrite(led1, LOW);
  delay(500);
  digitalWrite(led1, HIGH);
  delay(1000);
   digitalWrite(led1, LOW);
  delay(500);
  digitalWrite(led1, HIGH);
  delay(1000);
   digitalWrite(led1, LOW);
  delay(500);
  
  digitalWrite(led1, LOW);
  delay(1000);
  
  //For H
  digitalWrite(led1, HIGH);
  delay(1000);
   digitalWrite(led1, LOW);
  delay(500);
  digitalWrite(led1, HIGH);
  delay(1000);
   digitalWrite(led1, LOW);
  delay(500);
  digitalWrite(led1, HIGH);
  delay(1000);
   digitalWrite(led1, LOW);
  delay(500);
  digitalWrite(led1, HIGH);
  delay(1000);
   digitalWrite(led1, LOW);
  delay(500);
  
  digitalWrite(led1, LOW);
  delay(1000);
  
  // for E
  digitalWrite(led1, HIGH);
  delay(1000);
   digitalWrite(led1, LOW);
  delay(500);
  
   digitalWrite(led1, LOW);
  delay(1000);
  
  //For N
  digitalWrite(led1, HIGH);
  delay(2000);
  digitalWrite(led1, LOW);
  delay(500);
  digitalWrite(led1, HIGH);
  delay(1000);
   digitalWrite(led1, LOW);
  delay(500);
  
  
}
